<h2>Search Users</h2>
<form action="/users" method="GET">
    <label>Name</label>
    <input type = "text" name="name" value= "<%= searchOptions.name %>">
    <button type ="submit ">Search</button>
</form>
<br>
<% users.forEach(user => { %>
    <div><%= user.name %></div>
    <a href="/users/<%= user.name %>">View</a>
    <a href="/users/<%= user.name %>/edit">Edit</a>
    <%- include('../partials/deleteForm', 
        {url: `/users/${user.name}`})%>
<% })%>
